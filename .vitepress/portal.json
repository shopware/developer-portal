{
  "root": "src",
  "repositories": [
    {
      "name": "shopware/docs",
      "src": ".",
      "dst": "docs",
      "branch": [
        "env.BRANCH_DOCS",
        "main"
      ],
      "org": [
        "env.ORG_DOCS",
        "shopware"
      ]
    },
    {
      "name": "shopware/docs",
      "src": ".",
      "dst": [
        "docs",
        "v6.5"
      ],
      "branch": [
        "env.BRANCH_DOCS_65",
        "v6.5"
      ],
      "org": [
        "env.ORG_DOCS_65",
        "shopware"
      ]
    },
    {
      "name": "shopware/docs",
      "src": ".",
      "dst": [
        "docs",
        "v6.4"
      ],
      "branch": [
        "env.BRANCH_DOCS_64",
        "v6.4"
      ],
      "org": [
        "env.ORG_DOCS_64",
        "shopware"
      ]
    },
    {
      "name": "shopware/docs",
      "src": ".",
      "dst": [
        "docs",
        "v6.3"
      ],
      "branch": [
        "env.BRANCH_DOCS_63",
        "v6.3.0"
      ],
      "org": [
        "env.ORG_DOCS_63",
        "shopware"
      ]
    },
    {
      "name": "shopware/frontends",
      "src": [
        "apps",
        "docs",
        "src"
      ],
      "dst": "frontends",
      "branch": [
        "env.BRANCH_FRONTENDS",
        "DX-devhub"
      ],
      "org": [
        "env.ORG_FRONTENDS",
        "shopware"
      ]
    },
    {
      "name": "gitlab.shopware.com/product/engineering/platform-group/pwa/frontends",
      "src": [
        "apps",
        "docs",
        "src"
      ],
      "dst": "frontends-gl",
      "branch": [
        "env.BRANCH_FRONTENDS",
        "main"
      ],
      "org": [
        "env.ORG_FRONTENDS",
        "shopware"
      ],
      "env": {
        "GITLAB_FRONTENDS_USERNAME": {
          "as": "user",
          "description": "GitLab deploy key username"
        },
        "GITLAB_FRONTENDS_ACCESS_KEY": {
          "as": "pass",
          "description": "GitLab deploy key"
        }
      },
      "skip": true
    },
    {
      "name": "shopware/meteor",
      "src": [
        "packages",
        "admin-sdk",
        "docs"
      ],
      "dst": "resources/admin-extension-sdk",
      "branch": [
        "env.BRANCH_ADMIN_EXTENSION_SDK",
        "main"
      ],
      "org": [
        "env.ORG_ADMIN_EXTENSION_SDK",
        "shopware"
      ]
    },
    {
      "name": "shopware/meteor",
      "src": "packages/icon-kit/docs",
      "dst": "resources/meteor-icon-kit",
      "branch": [
        "env.BRANCH_METEOR_ICON_KIT",
        "main"
      ],
      "org": [
        "env.ORG_METEOR_ICON_KIT",
        "shopware"
      ],
      "env": {
        "FIGMA_FILE": {
          "description": "Figma file ID"
        },
        "FIGMA_TOKEN": {
          "description": "Figma API key"
        }
      }
    },
    {
      "name": "shopware/meteor-component-library",
      "src": "docs",
      "dst": "resources/meteor-component-library",
      "branch": [
        "env.BRANCH_METEOR_COMPONENT_LIBRARY",
        "DX-231",
        "main"
      ],
      "org": [
        "env.ORG_METEOR_COMPONENT_LIBRARY",
        "bojanrajh"
      ],
      "skip": true
    },
    {
      "name": "shopware/release-notes",
      "src": "src",
      "dst": "release-notes",
      "branch": [
        "env.BRANCH_RELEASE_NOTES",
        "main"
      ],
      "org": [
        "env.ORG_RELEASE_NOTES",
        "shopware"
      ],
      "skip": true
    }
  ]
}